{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-chrome",
            "request": "launch",
            //"preLaunchTask": "yarn: start",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:3000",
            "webRoot": "/opt/coias-front-app",
            "sourceMaps": true,
            //"name": "`yarn start`",
            "cwd": "/opt/coias-front-app",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "react-start"
            ],
            "sourceMapPathOverrides": {
                "webpack:///./*": "/opt/coias-front-app/src/*"
            }
        },
    ]
}
